# Configuração do Bot Discord
# Obtenha seu token do bot em https://discord.com/developers/applications
DISCORD_TOKEN=seu_token_do_bot_discord_aqui

# Configuração da API OpenRouter
# Obtenha sua chave da API em https://openrouter.ai/keys
OPENROUTER_API_KEY=sua_chave_api_openrouter_aqui

# Opcional: URL base do OpenRouter (padrão: https://openrouter.ai/api/v1)
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Opcional: Nível de log (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Opcional: Modelo de IA padrão a ser usado
MODEL_DEFAULT=openai/gpt-4o-mini

# Opcional: Habilitar sistema RAG (Retrieval-Augmented Generation)
RAG_ENABLED=false

# Opcional: Chave da API OpenAI (para recursos adicionais se necessário)
OPENAI_API_KEY=sua_chave_api_openai_aqui
